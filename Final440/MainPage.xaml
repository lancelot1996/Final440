<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Final440;assembly=Final440" x:Class="Final440.MainPage">

    <NavigationPage Title="Main" IconImageSource="main.png">
        <x:Arguments>
            <local:MainJobPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Schedule" IconImageSource="schedule.png">
        <x:Arguments>
            <local:SchedulePage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Service" IconImageSource="service.png">
            <x:Arguments>
            <local:ServicePage />
            </x:Arguments>
        </NavigationPage>
    <NavigationPage Title="Personal" IconImageSource="personal.png">
        <x:Arguments>
            <local:PersonalPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
